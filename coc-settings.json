{
  "dart.showTodos": false,
  "dart.enableSdkFormatter": false,
  "flutter.workspaceFolder.ignore": [
      "~/.pub-cache",
      "~/fvm/versions/stable/packages/flutter"
  ],
  "dart.analysisExcludedFolders": [
    "~/.pub-cache",
    "~/fvm/versions/stable/"
  ],
  "go.goplsOptions": {
    "completeUnimported": true,
    "hoverKind": "SingleLine",
  },
  "codeLens.enable": true,
  "go.goplsPath": "~/go/bin/gopls",
  "yaml.schemas": {
    "url": "globPattern",
    "Kubernetes": "globPattern"
  },
  "list.source.outline.defaultArgs": [],
  "rust-analyzer.server.path": "~/.cargo/bin/rust-analyzer",
  "notification.disabledProgressSources": [
    "*"
  ],
  "diagnostic.displayByAle": true,
  "coc.preferences.bracketEnterImprove": true,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.virtualText": false,
  "diagnostic.enable": true,
  "suggest.enablePreselect": false,
  "suggest.enableFloat": true,
  "suggest.virtualText": true,
  "suggest.noselect": false,
  "suggest.detailField": "preview",
  "suggest.minTriggerInputLength": 2,
  // override some settings specifically for dart
  "[dart]": {
    "suggest.triggerCompletionWait": 10,
    "suggest.minTriggerInputLength": 3,
    "suggest.timeout": 15000,
  },
  "hover.previewMaxHeight": 10,
  "sql.database": "postgresql",
  "sql.lintOnSave": false,
  "sql.lintOnChange": false,
  "suggest.completionItemKindLabels": {
    "keyword": "󰌋 ",
    "variable": " ",
    "value": " ",
    "operator": " ",
    "constructor": " ",
    "function": "󰊕 ",
    "reference": "  ",
    "constant": " ",
    "method": " ",
    "struct": " ",
    "class": " ",
    "interface": " ",
    "text": "󰊄 ",
    "enum": " ",
    "enumMember": " ",
    "module": " ",
    "color": " ",
    "property": "ﰠ ",
    "field": " ",
    "unit": "󱦒 ",
    "event": " ",
    "file": " ",
    "folder": " ",
    "snippet": " ",
    "typeParameter": " ",
    "default": ""
  },
  "hover.floatConfig": {
    "border": true,
    "title": " 🙈",
    "maxHeight": 10,
    "highlight": "TelescopeNormal",
    "borderhighlight": "TelescopeBorder",
  },
  "suggest.labelMaxLength": 50,
  "suggest.floatConfig": {
    "border": true,
    "highlight": "TelescopeNormal",
    "borderhighlight": "TelescopeBorder",
  },
  "signature.floatConfig": {
    "border": true,
    "title": " 🙈",
    "highlight": "TelescopeNormal",
    "borderhighlight": "TelescopeBorder",
  },
  "java.format.enabled": true,
  "java.format.comments.enabled": true,
  "java.format.onType.enabled": true,
  "snippets.extends": {
    "jet": [
      "html"
    ],
    "tmpl": [
      "html"
    ],
    "gohtmltmpl": [
      "html"
    ]
  },
  "snippets.ultisnips.pythonPrompt": false,
  "coc.preferences.extensionUpdateCheck": "never",
  "tsserver.log": "verbose",
  "tsserver.pluginPaths": ["."]
}
